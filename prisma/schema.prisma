generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id String @id @default(uuid())
  question String
  options String[]
  answer String
  responses Response[]
}

model Response {
  id String @id @default(uuid())
  questionId String
  response String
  question Question @relation(fields: [questionId], references: [id])
}